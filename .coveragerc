[run]
omit =
    */__init__.py
    */migrations/*
    ticket_manager/asgi.py
    ticket_manager/wsgi.py
    ticket_manager/settings.py
    */tests/*

[report]
exclude_lines =
    pragma: no cover
    def __str__
    if self\.debug
    if settings\.DEBUG
    if __name__ == .__main__.:

show_missing = True
